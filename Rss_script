-- ‚öΩ ENHANCED REALISTIC STREET SOCCER SCRIPT V3.0
-- üéØ –ê–≤—Ç–æ-–≥–æ–ª, —Ö–∏—Ç–±–æ–∫—Å—ã —Å —Ñ–∏–∑–∏–∫–æ–π, ESP, –∞–Ω—Ç–∏—á–∏—Ç, –º–æ–±–∏–ª—å–Ω–æ–µ –º–µ–Ω—é
-- ‚úÖ –†–∞–±–æ—Ç–∞–µ—Ç –Ω–∞ Delta –¥–ª—è –ü–ö –∏ —Ç–µ–ª–µ—Ñ–æ–Ω–æ–≤

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local HttpService = game:GetService("HttpService")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")

local lp = Players.LocalPlayer

print("=== üéØ REALISTIC STREET SOCCER ENHANCED LOADED ===")

-- ======================= –ù–ê–°–¢–†–û–ô–ö–ò =======================
local SETTINGS = {
    -- üéÆ –û—Å–Ω–æ–≤–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏
    AUTO_SCORE = true,
    AUTO_SCORE_KEY = Enum.KeyCode.F,
    AUTO_TACKLE = false,
    AUTO_TACKLE_RANGE = 15,
    
    -- üèÉ –ò–≥—Ä–æ–∫
    SPEED_BOOST = 1.5, -- –º–Ω–æ–∂–∏—Ç–µ–ª—å —Å–∫–æ—Ä–æ—Å—Ç–∏
    INFINITE_STAMINA = true,
    HIGH_JUMP = false,
    
    -- üéØ ESP
    BALL_ESP = true,
    BALL_ESP_COLOR = Color3.fromRGB(0, 255, 0),
    PLAYER_ESP = false,
    GOAL_ESP = true,
    
    -- ‚ö° –•–∏—Ç–±–æ–∫—Å—ã
    HITBOX_ENABLED = true,
    HITBOX_COLOR = Color3.fromRGB(0, 170, 255),
    HITBOX_TRANSPARENCY = 0.7,
    HITBOX_BOUNCE_FORCE = 1.2,
    
    -- üì± –ú–æ–±–∏–ª—å–Ω—ã–π –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å
    MOBILE_UI = true,
    MOBILE_BUTTON_SIZE = 60,
    
    -- üîí –ê–Ω—Ç–∏—á–∏—Ç
    ANTI_CHEAT = true,
    MAX_SPEED = 80,
    SPEED_CHECK_INTERVAL = 2
}

-- ======================= –ü–ï–†–ï–ú–ï–ù–ù–´–ï =======================
local ballCandidates = {}
local hitboxes = {}
local ballESP = {}
local mobileButtons = {}
local lastAutoScoreTime = 0
local autoScoreCooldown = 1

-- ======================= –ë–ê–ó–û–í–´–ï –§–£–ù–ö–¶–ò–ò =======================
function findBalls()
    ballCandidates = {}
    
    for _, obj in ipairs(Workspace:GetDescendants()) do
        if obj:IsA("BasePart") then
            local nameLower = obj.Name:lower()
            if nameLower:find("ball") or 
               nameLower:find("football") or 
               nameLower:find("soccer") or
               obj:GetAttribute("IsBall") or
               obj:FindFirstChild("BallScript") then
                table.insert(ballCandidates, obj)
            end
        end
    end
    
    -- –ï—Å–ª–∏ –Ω–µ –Ω–∞—à–ª–∏, —Å–æ–∑–¥–∞–µ–º –¥–µ—Ñ–æ–ª—Ç–Ω—ã–π –ø–æ–∏—Å–∫ –ø–æ —Ä–∞–∑–º–µ—Ä—É
    if #ballCandidates == 0 then
        for _, obj in ipairs(Workspace:GetDescendants()) do
            if obj:IsA("BasePart") and (obj.Size.X > 1.5 and obj.Size.X < 3) then
                table.insert(ballCandidates, obj)
            end
        end
    end
    
    warn("‚úÖ –ù–∞–π–¥–µ–Ω–æ –∫–∞–Ω–¥–∏–¥–∞—Ç–æ–≤ –Ω–∞ –º—è—á: " .. #ballCandidates)
    return ballCandidates
end

-- ======================= –§–£–ù–ö–¶–ò–Ø –•–ò–¢–ë–û–ö–°–û–í =======================
function createHitbox(position, size, name, options)
    if not SETTINGS.HITBOX_ENABLED then return nil end
    
    options = options or {}
    
    local hitbox = Instance.new("Part")
    hitbox.Name = name or "Hitbox_" .. HttpService:GenerateGUID(false)
    hitbox.Size = size or Vector3.new(10, 5, 1)
    hitbox.Position = position
    hitbox.Transparency = options.Transparency or SETTINGS.HITBOX_TRANSPARENCY
    hitbox.CanCollide = false
    hitbox.Anchored = true
    hitbox.Material = Enum.Material.Neon
    hitbox.Color = options.Color or SETTINGS.HITBOX_COLOR
    hitbox.CastShadow = false
    
    -- –ê—Ç—Ä–∏–±—É—Ç—ã –¥–ª—è –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ü–∏–∏
    hitbox:SetAttribute("IsHitbox", true)
    hitbox:SetAttribute("BounceForce", options.BounceForce or SETTINGS.HITBOX_BOUNCE_FORCE)
    
    -- –°–æ–∑–¥–∞–µ–º TouchTransmitter –¥–ª—è –æ–±–Ω–∞—Ä—É–∂–µ–Ω–∏—è –∫–∞—Å–∞–Ω–∏–π
    local touch = Instance.new("TouchTransmitter")
    touch.Parent = hitbox
    
    -- –ü–æ–¥—Å–≤–µ—Ç–∫–∞ —Ö–∏—Ç–±–æ–∫—Å–∞
    if options.Highlight ~= false then
        local highlight = Instance.new("Highlight")
        highlight.Name = "HitboxHighlight"
        highlight.FillColor = hitbox.Color
        highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
        highlight.FillTransparency = hitbox.Transparency + 0.2
        highlight.OutlineTransparency = 0
        highlight.Adornee = hitbox
        highlight.Parent = hitbox
    end
    
    hitbox.Parent = Workspace
    table.insert(hitboxes, hitbox)
    
    -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –∫–∞—Å–∞–Ω–∏—è –º—è—á–∞
    hitbox.Touched:Connect(function(hit)
        handleBallHit(hit, hitbox)
    end)
    
    return hitbox
end

function handleBallHit(hitPart, hitbox)
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ —ç—Ç–æ –º—è—á
    for _, ball in ipairs(ballCandidates) do
        if hitPart == ball or hitPart:IsDescendantOf(ball) then
            -- –í—ã—á–∏—Å–ª—è–µ–º –Ω–æ—Ä–º–∞–ª—å –æ—Ç—Å–∫–æ–∫–∞
            local hitNormal = (ball.Position - hitbox.Position).Unit
            
            -- –ü–æ–ª—É—á–∞–µ–º —Ç–µ–∫—É—â—É—é —Å–∫–æ—Ä–æ—Å—Ç—å –º—è—á–∞
            local velocity = ball.AssemblyLinearVelocity
            
            -- –ü—Ä–∏–º–µ–Ω—è–µ–º –æ—Ç—Å–∫–æ–∫ —Å —É—á—ë—Ç–æ–º —Å–∏–ª—ã —Ö–∏—Ç–±–æ–∫—Å–∞
            local bounceForce = hitbox:GetAttribute("BounceForce") or 1.2
            local newVelocity = velocity - (2 * velocity:Dot(hitNormal)) * hitNormal * bounceForce
            
            -- –ü—Ä–∏–º–µ–Ω—è–µ–º –Ω–æ–≤—É—é —Å–∫–æ—Ä–æ—Å—Ç—å
            ball.AssemblyLinearVelocity = newVelocity
            
            -- –í–∏–∑—É–∞–ª—å–Ω—ã–π —ç—Ñ—Ñ–µ–∫—Ç
            spawn(function()
                if hitbox:FindFirstChild("HitboxHighlight") then
                    local originalColor = hitbox.HitboxHighlight.FillColor
                    hitbox.HitboxHighlight.FillColor = Color3.fromRGB(255, 50, 50)
                    
                    wait(0.1)
                    
                    if hitbox and hitbox.Parent then
                        hitbox.HitboxHighlight.FillColor = originalColor
                    end
                end
            end)
            
            break
        end
    end
end

function setupFieldHitboxes()
    -- –û—á–∏—â–∞–µ–º —Å—Ç–∞—Ä—ã–µ —Ö–∏—Ç–±–æ–∫—Å—ã
    for _, hb in ipairs(hitboxes) do
        if hb and hb.Parent then
            hb:Destroy()
        end
    end
    hitboxes = {}
    
    -- –û–ø—Ä–µ–¥–µ–ª—è–µ–º —Ä–∞–∑–º–µ—Ä—ã –ø–æ–ª—è
    local fieldCenter = Vector3.new(0, 5, 0)
    local fieldWidth = 120
    local fieldLength = 80
    
    -- –ë–æ–∫–æ–≤—ã–µ —Å—Ç–µ–Ω–∫–∏
    createHitbox(
        fieldCenter + Vector3.new(0, 3, fieldLength/2), 
        Vector3.new(fieldWidth, 8, 1), 
        "Wall_North",
        {BounceForce = 1.3}
    )
    
    createHitbox(
        fieldCenter + Vector3.new(0, 3, -fieldLength/2), 
        Vector3.new(fieldWidth, 8, 1), 
        "Wall_South",
        {BounceForce = 1.3}
    )
    
    createHitbox(
        fieldCenter + Vector3.new(fieldWidth/2, 3, 0), 
        Vector3.new(1, 8, fieldLength), 
        "Wall_East",
        {BounceForce = 1.3}
    )
    
    createHitbox(
        fieldCenter + Vector3.new(-fieldWidth/2, 3, 0), 
        Vector3.new(1, 8, fieldLength), 
        "Wall_West",
        {BounceForce = 1.3}
    )
    
    -- –¶–µ–Ω—Ç—Ä–∞–ª—å–Ω—ã–µ –ø—Ä–µ–ø—è—Ç—Å—Ç–≤–∏—è (–∫–∞–∫ –Ω–∞ –≤–∞—à–∏—Ö —Å–∫—Ä–∏–Ω—à–æ—Ç–∞—Ö)
    createHitbox(
        fieldCenter + Vector3.new(15, 2, 15), 
        Vector3.new(8, 5, 1), 
        "Obstacle_1",
        {Color = Color3.fromRGB(255, 100, 0), BounceForce = 1.5}
    )
    
    createHitbox(
        fieldCenter + Vector3.new(-15, 2, -15), 
        Vector3.new(1, 5, 8), 
        "Obstacle_2",
        {Color = Color3.fromRGB(255, 100, 0), BounceForce = 1.5}
    )
    
    -- –í–æ—Ä–æ—Ç–∞ (–±–æ–ª–µ–µ —Ç–æ–Ω–∫–∏–µ)
    createHitbox(
        Vector3.new(0, 3, fieldLength/2 - 2), 
        Vector3.new(20, 8, 0.5), 
        "Goal_North",
        {Color = Color3.fromRGB(0, 255, 0), Transparency = 0.8, BounceForce = 1.8}
    )
    
    createHitbox(
        Vector3.new(0, 3, -fieldLength/2 + 2), 
        Vector3.new(20, 8, 0.5), 
        "Goal_South",
        {Color = Color3.fromRGB(0, 255, 0), Transparency = 0.8, BounceForce = 1.8}
    )
    
    warn("‚úÖ –•–∏—Ç–±–æ–∫—Å—ã —Å–æ–∑–¥–∞–Ω—ã: " .. #hitboxes .. " —à—Ç—É–∫")
end

-- ======================= BALL ESP (–£–õ–£–ß–®–ï–ù–ù–´–ô) =======================
function setupBallESP()
    -- –û—á–∏—â–∞–µ–º —Å—Ç–∞—Ä—ã–π ESP
    for _, esp in pairs(ballESP) do
        if esp and esp.Parent then
            esp:Destroy()
        end
    end
    ballESP = {}
    
    if not SETTINGS.BALL_ESP then return end
    
    for _, ball in ipairs(ballCandidates) do
        -- –ü–æ–¥—Å–≤–µ—Ç–∫–∞ –º—è—á–∞
        local highlight = Instance.new("Highlight")
        highlight.Name = "BallESP_Highlight"
        highlight.FillColor = SETTINGS.BALL_ESP_COLOR
        highlight.OutlineColor = Color3.fromRGB(255, 255, 0)
        highlight.FillTransparency = 0.6
        highlight.OutlineTransparency = 0
        highlight.Adornee = ball
        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
        highlight.Parent = ball
        
        -- –¢–µ–∫—Å—Ç–æ–≤–∞—è –º–µ—Ç–∫–∞
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "BallESP_Billboard"
        billboard.Size = UDim2.new(0, 200, 0, 50)
        billboard.AlwaysOnTop = true
        billboard.MaxDistance = 500
        billboard.Adornee = ball
        billboard.Parent = ball
        
        local label = Instance.new("TextLabel")
        label.Name = "BallLabel"
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.TextScaled = true
        label.Font = Enum.Font.GothamBold
        label.TextColor3 = Color3.fromRGB(255, 255, 0)
        label.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
        label.TextStrokeTransparency = 0.3
        label.Text = "‚öΩ " .. ball.Name
        label.Parent = billboard
        
        -- –î–∏—Å—Ç–∞–Ω—Ü–∏—è (–æ–±–Ω–æ–≤–ª—è–µ—Ç—Å—è –≤ —Ä–µ–∞–ª—å–Ω–æ–º –≤—Ä–µ–º–µ–Ω–∏)
        local distLabel = Instance.new("TextLabel")
        distLabel.Name = "DistanceLabel"
        distLabel.Size = UDim2.new(1, 0, 0, 20)
        distLabel.Position = UDim2.new(0, 0, 1, 0)
        distLabel.BackgroundTransparency = 1
        distLabel.TextScaled = true
        distLabel.Font = Enum.Font.Gotham
        distLabel.TextColor3 = Color3.fromRGB(200, 200, 255)
        distLabel.TextStrokeTransparency = 0.3
        distLabel.Text = "0 studs"
        distLabel.Parent = billboard
        
        table.insert(ballESP, {highlight = highlight, billboard = billboard, ball = ball})
    end
end

function updateBallESP()
    local char = lp.Character
    local root = char and char:FindFirstChild("HumanoidRootPart")
    if not root then return end
    
    for _, espData in ipairs(ballESP) do
        if espData.ball and espData.ball.Parent and espData.billboard.Parent then
            local dist = (espData.ball.Position - root.Position).Magnitude
            local distLabel = espData.billboard:FindFirstChild("DistanceLabel")
            if distLabel then
                distLabel.Text = string.format("%.1f studs", dist)
                
                -- –ú–µ–Ω—è–µ–º —Ü–≤–µ—Ç –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç —Ä–∞—Å—Å—Ç–æ—è–Ω–∏—è
                if dist < 10 then
                    espData.highlight.FillColor = Color3.fromRGB(0, 255, 0)
                elseif dist < 30 then
                    espData.highlight.FillColor = Color3.fromRGB(255, 255, 0)
                else
                    espData.highlight.FillColor = Color3.fromRGB(255, 50, 50)
                end
            end
        end
    end
end

-- ======================= –ê–í–¢–û-–ì–û–õ =======================
function autoScore()
    if not SETTINGS.AUTO_SCORE then return end
    if tick() - lastAutoScoreTime < autoScoreCooldown then return end
    
    local char = lp.Character
    local root = char and char:FindFirstChild("HumanoidRootPart")
    if not root then return end
    
    -- –ò—â–µ–º –±–ª–∏–∂–∞–π—à–∏–π –º—è—á
    local closestBall = nil
    local closestDist = math.huge
    
    for _, ball in ipairs(ballCandidates) do
        if ball and ball.Parent then
            local dist = (ball.Position - root.Position).Magnitude
            if dist < closestDist and dist < 50 then
                closestDist = dist
                closestBall = ball
            end
        end
    end
    
    if closestBall then
        -- –û–ø—Ä–µ–¥–µ–ª—è–µ–º –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –∫ –±–ª–∏–∂–∞–π—à–∏–º –≤–æ—Ä–æ—Ç–∞–º
        local goalPositions = {
            Vector3.new(0, 3, 40),   -- –°–µ–≤–µ—Ä–Ω—ã–µ –≤–æ—Ä–æ—Ç–∞
            Vector3.new(0, 3, -40)   -- –Æ–∂–Ω—ã–µ –≤–æ—Ä–æ—Ç–∞
        }
        
        local targetGoal = goalPositions[1]
        if (root.Position - goalPositions[2]).Magnitude < (root.Position - goalPositions[1]).Magnitude then
            targetGoal = goalPositions[2]
        end
        
        -- –ù–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ —É–¥–∞—Ä–∞
        local direction = (targetGoal - closestBall.Position).Unit
        
        -- –ü—Ä–∏–º–µ–Ω—è–µ–º —Å–∏–ª—É –∫ –º—è—á—É
        local bodyVelocity = closestBall:FindFirstChild("BodyVelocity") or Instance.new("BodyVelocity")
        bodyVelocity.Velocity = direction * 80
        bodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
        bodyVelocity.P = 1000
        bodyVelocity.Parent = closestBall
        
        lastAutoScoreTime = tick()
        
        -- –í–∏–∑—É–∞–ª—å–Ω—ã–π —ç—Ñ—Ñ–µ–∫—Ç —É–¥–∞—Ä–∞
        spawn(function()
            if closestBall:FindFirstChild("BallESP_Highlight") then
                local originalColor = closestBall.BallESP_Highlight.FillColor
                closestBall.BallESP_Highlight.FillColor = Color3.fromRGB(255, 0, 0)
                
                wait(0.2)
                
                if closestBall and closestBall.Parent then
                    closestBall.BallESP_Highlight.FillColor = originalColor
                end
            end
        end)
        
        print("‚úÖ –ê–≤—Ç–æ-–≥–æ–ª –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω! –ú—è—á –ª–µ—Ç–∏—Ç –∫ –≤–æ—Ä–æ—Ç–∞–º.")
    end
end

-- ======================= –£–ü–†–ê–í–õ–ï–ù–ò–ï –ò–ì–†–û–ö–û–ú =======================
function applyPlayerMods()
    local char = lp.Character
    if not char then return end
    
    local humanoid = char:FindFirstChild("Humanoid")
    if humanoid then
        -- –°–∫–æ—Ä–æ—Å—Ç—å
        humanoid.WalkSpeed = 16 * SETTINGS.SPEED_BOOST
        
        -- –ü—Ä—ã–∂–æ–∫
        if SETTINGS.HIGH_JUMP then
            humanoid.JumpPower = 60
        end
        
        -- –ë–µ—Å–∫–æ–Ω–µ—á–Ω–∞—è –≤—ã–Ω–æ—Å–ª–∏–≤–æ—Å—Ç—å
        if SETTINGS.INFINITE_STAMINA then
            pcall(function()
                humanoid:SetAttribute("Stamina", 100)
            end)
        end
    end
end

-- ======================= –ú–û–ë–ò–õ–¨–ù–´–ô –ò–ù–¢–ï–†–§–ï–ô–° =======================
function createMobileUI()
    if not SETTINGS.MOBILE_UI or not UserInputService.TouchEnabled then return end
    
    -- –û—á–∏—â–∞–µ–º —Å—Ç–∞—Ä—ã–µ –∫–Ω–æ–ø–∫–∏
    for _, btn in ipairs(mobileButtons) do
        if btn and btn.Parent then
            btn:Destroy()
        end
    end
    mobileButtons = {}
    
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "MobileSoccerUI"
    screenGui.ResetOnSpawn = false
    screenGui.Parent = lp:WaitForChild("PlayerGui")
    
    local buttonTemplates = {
        {
            Name = "SCORE",
            Position = UDim2.new(1, -70, 0.5, -30),
            Color = Color3.fromRGB(0, 200, 0),
            Action = function()
                autoScore()
            end
        },
        {
            Name = "SPEED+",
            Position = UDim2.new(1, -70, 0.6, -30),
            Color = Color3.fromRGB(0, 150, 255),
            Action = function()
                SETTINGS.SPEED_BOOST = math.min(3, SETTINGS.SPEED_BOOST + 0.2)
                applyPlayerMods()
            end
        },
        {
            Name = "SPEED-",
            Position = UDim2.new(1, -70, 0.7, -30),
            Color = Color3.fromRGB(255, 100, 0),
            Action = function()
                SETTINGS.SPEED_BOOST = math.max(1, SETTINGS.SPEED_BOOST - 0.2)
                applyPlayerMods()
            end
        },
        {
            Name = "HITBOX",
            Position = UDim2.new(0, 10, 0.5, -30),
            Color = Color3.fromRGB(0, 170, 255),
            Action = function()
                SETTINGS.HITBOX_ENABLED = not SETTINGS.HITBOX_ENABLED
                setupFieldHitboxes()
            end
        },
        {
            Name = "ESP",
            Position = UDim2.new(0, 10, 0.6, -30),
            Color = Color3.fromRGB(255, 255, 0),
            Action = function()
                SETTINGS.BALL_ESP = not SETTINGS.BALL_ESP
                setupBallESP()
            end
        }
    }
    
    for _, template in ipairs(buttonTemplates) do
        local button = Instance.new("TextButton")
        button.Name = template.Name
        button.Size = UDim2.new(0, SETTINGS.MOBILE_BUTTON_SIZE, 0, SETTINGS.MOBILE_BUTTON_SIZE)
        button.Position = template.Position
        button.BackgroundColor3 = template.Color
        button.BackgroundTransparency = 0.2
        button.Text = template.Name
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        button.Font = Enum.Font.GothamBold
        button.TextSize = 16
        button.BorderSizePixel = 0
        
        -- –≠—Ñ—Ñ–µ–∫—Ç –ø—Ä–∏ –Ω–∞–∂–∞—Ç–∏–∏
        button.MouseButton1Down:Connect(function()
            button.BackgroundTransparency = 0.5
        end)
        
        button.MouseButton1Up:Connect(function()
            button.BackgroundTransparency = 0.2
            template.Action()
        end)
        
        button.Parent = screenGui
        table.insert(mobileButtons, button)
    end
    
    -- –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏–æ–Ω–Ω–∞—è –ø–∞–Ω–µ–ª—å
    local infoFrame = Instance.new("Frame")
    infoFrame.Name = "InfoPanel"
    infoFrame.Size = UDim2.new(1, -20, 0, 80)
    infoFrame.Position = UDim2.new(0, 10, 0, 10)
    infoFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    infoFrame.BackgroundTransparency = 0.5
    infoFrame.BorderSizePixel = 0
    infoFrame.Parent = screenGui
    
    local title = Instance.new("TextLabel")
    title.Name = "Title"
    title.Size = UDim2.new(1, 0, 0, 30)
    title.Position = UDim2.new(0, 0, 0, 0)
    title.BackgroundTransparency = 1
    title.Text = "‚öΩ RSS ENHANCED V3.0"
    title.TextColor3 = Color3.fromRGB(0, 255, 255)
    title.Font = Enum.Font.GothamBold
    title.TextSize = 18
    title.Parent = infoFrame
    
    local status = Instance.new("TextLabel")
    status.Name = "Status"
    status.Size = UDim2.new(1, 0, 0, 50)
    status.Position = UDim2.new(0, 0, 0, 30)
    status.BackgroundTransparency = 1
    status.Text = "–ú—è—á–∏: 0 | –•–∏—Ç–±–æ–∫—Å—ã: –í–´–ö–õ"
    status.TextColor3 = Color3.fromRGB(255, 255, 255)
    status.Font = Enum.Font.Gotham
    status.TextSize = 14
    status.Parent = infoFrame
    
    table.insert(mobileButtons, infoFrame)
    
    warn("üì± –ú–æ–±–∏–ª—å–Ω—ã–π –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å —Å–æ–∑–¥–∞–Ω")
end

-- ======================= –ê–ù–¢–ò–ß–ò–¢ –°–ò–°–¢–ï–ú–ê =======================
function setupAntiCheat()
    if not SETTINGS.ANTI_CHEAT then return end
    
    spawn(function()
        while wait(SETTINGS.SPEED_CHECK_INTERVAL) do
            local char = lp.Character
            if char then
                local humanoid = char:FindFirstChild("Humanoid")
                if humanoid and humanoid.WalkSpeed > SETTINGS.MAX_SPEED then
                    warn("‚ö†Ô∏è –ê–Ω—Ç–∏—á–∏—Ç: –û–±–Ω–∞—Ä—É–∂–µ–Ω–∞ –ø–æ–¥–æ–∑—Ä–∏—Ç–µ–ª—å–Ω–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å")
                    humanoid.WalkSpeed = 32
                end
            end
        end
    end)
    
    -- –ó–∞—â–∏—Ç–∞ –æ—Ç —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–π
    local lastPosition = nil
    spawn(function()
        while wait(1) do
            local char = lp.Character
            if char then
                local root = char:FindFirstChild("HumanoidRootPart")
                if root then
                    if lastPosition then
                        local distance = (root.Position - lastPosition).Magnitude
                        if distance > 100 then -- –ú–∞–∫—Å–∏–º—É–º 100 studs –≤ —Å–µ–∫—É–Ω–¥—É
                            warn("‚ö†Ô∏è –ê–Ω—Ç–∏—á–∏—Ç: –í–æ–∑–º–æ–∂–Ω–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è")
                        end
                    end
                    lastPosition = root.Position
                end
            end
        end
    end)
end

-- ======================= –ì–û–†–Ø–ß–ò–ï –ö–õ–ê–í–ò–®–ò =======================
function setupHotkeys()
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if gameProcessed then return end
        
        -- –ê–≤—Ç–æ-–≥–æ–ª –Ω–∞ F
        if input.KeyCode == SETTINGS.AUTO_SCORE_KEY then
            autoScore()
        end
        
        -- –ü–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏–µ ESP –Ω–∞ H
        if input.KeyCode == Enum.KeyCode.H then
            SETTINGS.BALL_ESP = not SETTINGS.BALL_ESP
            setupBallESP()
            warn("üì° Ball ESP: " .. (SETTINGS.BALL_ESP and "–í–ö–õ" or "–í–´–ö–õ"))
        end
        
        -- –ü–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏–µ —Ö–∏—Ç–±–æ–∫—Å–æ–≤ –Ω–∞ J
        if input.KeyCode == Enum.KeyCode.J then
            SETTINGS.HITBOX_ENABLED = not SETTINGS.HITBOX_ENABLED
            setupFieldHitboxes()
            warn("üéØ –•–∏—Ç–±–æ–∫—Å—ã: " .. (SETTINGS.HITBOX_ENABLED and "–í–ö–õ" or "–í–´–ö–õ"))
        end
        
        -- –°–±—Ä–æ—Å —Ö–∏—Ç–±–æ–∫—Å–æ–≤ –Ω–∞ R
        if input.KeyCode == Enum.KeyCode.R then
            setupFieldHitboxes()
            warn("üîÑ –•–∏—Ç–±–æ–∫—Å—ã –ø–µ—Ä–µ—Å–æ–∑–¥–∞–Ω—ã")
        end
    end)
end

-- ======================= –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø =======================
function initialize()
    print("\n=== –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø –°–ö–†–ò–ü–¢–ê ===")
    
    -- 1. –ò—â–µ–º –º—è—á–∏
    findBalls()
    
    -- 2. –ù–∞—Å—Ç—Ä–∞–∏–≤–∞–µ–º ESP –¥–ª—è –º—è—á–µ–π
    setupBallESP()
    
    -- 3. –°–æ–∑–¥–∞–µ–º —Ö–∏—Ç–±–æ–∫—Å—ã
    setupFieldHitboxes()
    
    -- 4. –ù–∞—Å—Ç—Ä–∞–∏–≤–∞–µ–º –∞–Ω—Ç–∏—á–∏—Ç
    setupAntiCheat()
    
    -- 5. –°–æ–∑–¥–∞–µ–º –º–æ–±–∏–ª—å–Ω—ã–π –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å
    createMobileUI()
    
    -- 6. –ù–∞—Å—Ç—Ä–∞–∏–≤–∞–µ–º –≥–æ—Ä—è—á–∏–µ –∫–ª–∞–≤–∏—à–∏
    setupHotkeys()
    
    print("\n‚úÖ –°–ö–†–ò–ü–¢ –£–°–ü–ï–®–ù–û –ó–ê–ì–†–£–ñ–ï–ù!")
    print("üéÆ –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ:")
    print("   F - –ê–≤—Ç–æ-–≥–æ–ª")
    print("   H - –í–∫–ª/–í—ã–∫–ª ESP –º—è—á–∞")
    print("   J - –í–∫–ª/–í—ã–∫–ª —Ö–∏—Ç–±–æ–∫—Å—ã")
    print("   R - –ü–µ—Ä–µ—Å–æ–∑–¥–∞—Ç—å —Ö–∏—Ç–±–æ–∫—Å—ã")
    print("üì± –ú–æ–±–∏–ª—å–Ω—ã–µ –∫–Ω–æ–ø–∫–∏ –∞–∫—Ç–∏–≤–Ω—ã")
end

-- ======================= –ì–õ–ê–í–ù–´–ô –¶–ò–ö–õ =======================
spawn(function()
    initialize()
    
    RunService.Heartbeat:Connect(function(deltaTime)
        -- –û–±–Ω–æ–≤–ª—è–µ–º ESP –¥–∏—Å—Ç–∞–Ω—Ü–∏–∏
        updateBallESP()
        
        -- –ü—Ä–∏–º–µ–Ω—è–µ–º –º–æ–¥–∏—Ñ–∏–∫–∞—Ü–∏–∏ –∏–≥—Ä–æ–∫–∞
        applyPlayerMods()
        
        -- –ê–≤—Ç–æ-–æ—Ç–±–æ—Ä (–µ—Å–ª–∏ –≤–∫–ª—é—á–µ–Ω)
        if SETTINGS.AUTO_TACKLE then
            -- –õ–æ–≥–∏–∫–∞ –∞–≤—Ç–æ-–æ—Ç–±–æ—Ä–∞
        end
    end)
end)

-- –ê–≤—Ç–æ-–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –º—è—á–µ–π
spawn(function()
    while wait(5) do
        local oldCount = #ballCandidates
        findBalls()
        
        if #ballCandidates ~= oldCount then
            setupBallESP()
            warn("üîÑ –û–±–Ω–æ–≤–ª–µ–Ω —Å–ø–∏—Å–æ–∫ –º—è—á–µ–π: " .. #ballCandidates .. " –Ω–∞–π–¥–µ–Ω–æ")
        end
    end
end)

-- –ê–Ω—Ç–∏-–ê–§–ö
local VirtualUser = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:Connect(function()
    VirtualUser:Button2Down(Vector2.new(0,0), Workspace.CurrentCamera.CFrame)
    wait(1)
    VirtualUser:Button2Up(Vector2.new(0,0), Workspace.CurrentCamera.CFrame)
end)

print("\n=== üéØ –°–ò–°–¢–ï–ú–ê –ì–û–¢–û–í–ê –ö –†–ê–ë–û–¢–ï ===")
print("üéØ –ù–∞–π–¥–µ–Ω–æ –º—è—á–µ–π: " .. #ballCandidates)
print("üéØ –°–æ–∑–¥–∞–Ω–æ —Ö–∏—Ç–±–æ–∫—Å–æ–≤: " .. #hitboxes)
print("üì± –ú–æ–±–∏–ª—å–Ω—ã–π –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å: " .. (UserInputService.TouchEnabled and "–ê–ö–¢–ò–í–ï–ù" or "–ù–ï –ê–ö–¢–ò–í–ï–ù"))
print("üîí –ê–Ω—Ç–∏—á–∏—Ç —Å–∏—Å—Ç–µ–º–∞: " .. (SETTINGS.ANTI_CHEAT and "–í–ö–õ" or "–í–´–ö–õ"))
print("\n‚öΩ –ü—Ä–∏—è—Ç–Ω–æ–π –∏–≥—Ä—ã –≤ Realistic Street Soccer!")

-- –í–æ–∑–≤—Ä–∞—â–∞–µ–º —É–ø—Ä–∞–≤–ª—è—é—â–∏–µ —Ñ—É–Ω–∫—Ü–∏–∏
return {
    FindBalls = findBalls,
    SetupHitboxes = setupFieldHitboxes,
    ToggleESP = function()
        SETTINGS.BALL_ESP = not SETTINGS.BALL_ESP
        setupBallESP()
    end,
    AutoScore = autoScore,
    GetSettings = function() return SETTINGS end,
    UpdateSettings = function(newSettings)
        for k, v in pairs(newSettings) do
            if SETTINGS[k] ~= nil then
                SETTINGS[k] = v
            end
        end
        applyPlayerMods()
    end
}
