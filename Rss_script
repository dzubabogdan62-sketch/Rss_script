        end
    end
})

local DashboardSection = SettingsTab:Section({
    Title = "üì± –ü–∞–Ω–µ–ª—å —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è"
})

DashboardSection:Button({
    Title = "üî• –ü–û–õ–ù–ê–Ø –°–¢–ê–¢–ò–°–¢–ò–ö–ê",
    Callback = function()
        local accuracy, totalRating = GetGoalkeeperRating()
        local dangerLevel = CalculateDangerLevel()
        local ping = GetPlayerPing()
        
        local dashboard = "‚ïî‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïó\n"
        dashboard = dashboard .. "‚ïë   üìä DASHBOARD RSS   ‚ïë\n"
        dashboard = dashboard .. "‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù\n\n"
        dashboard = dashboard .. "üéØ –°–¢–ê–¢–ò–°–¢–ò–ö–ê –î–ê–ô–í–û–í:\n"
        dashboard = dashboard .. "  –í—Å–µ–≥–æ: " .. AnalyticsState.TotalDives .. " | "
        dashboard = dashboard .. "‚úÖ: " .. AnalyticsState.SuccessfulDives .. " | "
        dashboard = dashboard .. "‚ùå: " .. AnalyticsState.FailedDives .. "\n"
        dashboard = dashboard .. "  –¢–æ—á–Ω–æ—Å—Ç—å: " .. math.floor(accuracy) .. "%\n\n"
        dashboard = dashboard .. "‚≠ê –†–ï–ô–¢–ò–ù–ì: " .. totalRating .. "/10\n\n"
        dashboard = dashboard .. "üö® –û–ü–ê–°–ù–û–°–¢–¨: " .. dangerLevel .. "/100\n"
        dashboard = dashboard .. "üì° –ü–ò–ù–ì: " .. ping .. "ms\n\n"
        dashboard = dashboard .. "üéÆ –†–ï–ñ–ò–ú: " .. (ClutchMode.Enabled and "üéØ –ö–õ–ê–¢–ß" or "–û–ë–´–ß–ù–´–ô") .. "\n"
        
        print(dashboard)
        Notify("Dashboard", dashboard, "check")
    end
})

DashboardSection:Button({
    Title = "üíæ –ë—ã—Å—Ç—Ä–æ–µ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –ø—Ä–æ—Ñ–∏–ª—è",
    Callback = function()
        local profileData = {
            timestamp = os.time(),
            totalDives = AnalyticsState.TotalDives,
            successfulDives = AnalyticsState.SuccessfulDives,
            accuracy = AnalyticsState.AverageDiveAccuracy,
            settings = SavedSettings,
        }
        
        local json = game:GetService("HttpService"):JSONEncode(profileData)
        pcall(function()
            writefile("RSS_PROFILE_" .. os.time() .. ".json", json)
        end)
        
        Notify("–°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ", "–ü—Ä–æ—Ñ–∏–ª—å —Å–æ—Ö—Ä–∞–Ω—ë–Ω!", "check")
    end
})

DashboardSection:Button({
    Title = "‚ö° –û–±—â–∏–π —Å—Ç–∞—Ç—É—Å –≤—Å–µ—Ö —Å–∏—Å—Ç–µ–º",
    Callback = function()
        local status = "‚ïî‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïó\n"
        status = status .. "‚ïë   –°–¢–ê–¢–£–° –°–ò–°–¢–ï–ú   ‚ïë\n"
        status = status .. "‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù\n\n"
        status = status .. "‚úì Reach: " .. (State.IsReachEnabled and "–í–ö–õ" or "–í–´–ö–õ") .. "\n"
        status = status .. "‚úì Smart Dive: " .. (DiveState.Enabled and "–í–ö–õ" or "–í–´–ö–õ") .. "\n"
        status = status .. "‚úì Advanced Dive: " .. (AdvancedDiveState.Enabled and "–í–ö–õ" or "–í–´–ö–õ") .. "\n"
        status = status .. "‚úì Ball Predictor: " .. (PredictionState.Enabled and "–í–ö–õ" or "–í–´–ö–õ") .. "\n"
        status = status .. "‚úì Stamina: " .. (State.InfiniteStaminaEnabled and "–í–ö–õ" or "–í–´–ö–õ") .. "\n"
        status = status .. "‚úì Clutch Mode: " .. (ClutchMode.Enabled and "–í–ö–õ üéØ" or "–í–´–ö–õ") .. "\n\n"
        status = status .. "üì° –ü–∏–Ω–≥: " .. GetPlayerPing() .. "ms\n"
        status = status .. "üåê FPS: " .. math.floor(1 / game:GetService("RunService").Heartbeat:Wait()) .. "\n"
        
        print(status)
        Notify("–°–∏—Å—Ç–µ–º—ã", status, "check")
    end
})

-- ============ –ó–ê–í–ï–†–®–ï–ù–ò–ï ============

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
CreateHitbox()
UpdateHitbox()

-- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ —Å–æ—Å—Ç–æ—è–Ω–∏—è –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ
task.wait(1)
if SavedSettings.ReachEnabled then
    EnableReach()
    Notify("–í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ", "Reach –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –≤–∫–ª—é—á–µ–Ω", "check")
end

if SavedSettings.InfiniteStaminaEnabled then
    State.InfiniteStaminaEnabled = true
    getgenv().InfiniteStaminaEnabled = true
    UpdateStamina()
    Notify("–í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ", "Stamina –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –≤–∫–ª—é—á–µ–Ω–∞", "check")
end

